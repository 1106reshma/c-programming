//program to check if number is fiboncii or not
#include<stdio.h>
int main()
    {
      int f1,f2,f3,n;
      printf("Enter the number to check:");
      scanf("%d",&n);
      f1=0;
      f2=1;
      if(n==0)
      printf("Fiboncii series");
      f3=f1+f2;
      while(f3<n)
      {
          f1=f2;
          f2=f3;
          f3=f1+f2;
      }
      if(f3==n)
      printf("YES");
      else
      printf("NO");
      return 0;
 }
 
 INPUT 1:
 Enter the number to check:5
 OUTPUT:YES
 
 INPUT 2:
 Enter the number to check:6
 OUTPUT:NO
 
 
